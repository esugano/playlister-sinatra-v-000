<!-- Displays Artist's Songs & Link -->
<<p>Songs:
  <%@slug.songs.each do |song|%>
  <li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li>
  <%end%>
</p>
<!-- Displays Artist's Genres & Link -->
<p>Genres:
  <%@slug.genres.each do |genre|%>
  <li><a href="/genres/<%=genre.slug%>"><%=genre.name%></a></li>
  <%end%>
</p>
