<!-- Displays Genre's Songs & Link -->
<<p>Songs:
  <%@slug.songs.each do |song|%>
  <li><a href="/songs/<%=song.slug%>"><%=song.name%></a></li>
  <%end%>
</p>
<!-- Displays Genre's Artists & Link -->
<p>Artists:
  <%@slug.artists.each do |artist|%>
  <li><a href="/artists/<%=artist.slug%>"><%=artist.name%></a></li>
  <%end%>
</p>
