<h1>Create a new song</h1>
<form action="/songs" method="POST">
  <label for="song_name">Song Name:</label>
  <input type="text" name="song_name" id="song_name"></input>
  <br></br>
  <label for="artist_name">Artist Name:</label>
  <%@artists.each do |artist|%>
    <input type="checkbox" name="owner[pet_ids][]" value="<%=pet.id%>" id="<%=pet.id%>"><%=artist.name%></input>
  <%end%>
  <input type="text" name="artist_name" id="artist_name"></input>
  <br></br>
  <label for="genre_name">Genre Name:</label>
  <input type="text" name="genre_name" id="genre_name"></input>
  <br></br>
  <input type="submit" value="Create">
</form>
